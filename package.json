{
  "name": "@muritavo/cypress-toolkit",
  "version": "0.16.4",
  "description": "A series of utility functions for working with cypress (mainly cypress Component testing)",
  "main": "dist/index.js",
  "repository": "git@github.com:Muritavo/cypress-toolkit.git",
  "author": "Murilo Araujo <murilo.araujo@onepercent.io>",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "unit testing",
    "blockchain",
    "firebase",
    "automation"
  ],
  "type": "module",
  "devDependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@muritavo/webpack-microfrontend-scripts": "^0.1.3",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@types/debug": "^4.1.12",
    "@types/kill-port": "^2.0.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/spellchecker": "^3.5.0",
    "@types/webpack": "^5.28.0",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-istanbul": "^7.0.1",
    "cypress": "15.3.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.7.0",
    "cypress-visual-regression": "https://github.com/muritavo/cypress-visual-regression",
    "cypress-wait-until": "^3.0.2",
    "ethers": "^6.15.0",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "ganache": "^7.3.2",
    "hardhat": "3.0.6",
    "jest": "^30.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "spellchecker": "^3.7.1",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.28.13",
    "typedoc-plugin-merge-modules": "^7.0.0",
    "typescript": "^5.0.3"
  },
  "scripts": {
    "doc": "typedoc ./index.d.ts ./types/ai.d.ts --excludeExternals --excludeInternal --disableSources",
    "dev": "tsc --watch",
    "dev:vite": "tsc -P ./tsconfig.vite.json --watch",
    "prebuild": "yarn doc",
    "build": "tsc",
    "test:watch": "cypress open --component --browser chrome",
    "test:e2e:watch": "cypress open --e2e --browser chrome",
    "prepack": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll"
  },
  "types": "index.d.ts",
  "dependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@cypress/react": "^9.0.1",
    "@cypress/snapshot": "^2.1.7",
    "@muritavo/testing-toolkit": "^0.6.0",
    "@types/find-cache-dir": "^5.0.2",
    "@types/lodash": "^4.17.0",
    "@types/node-fetch": "^2.6.2",
    "debug": "^4.3.4",
    "dot-notes": "^3.2.1",
    "find-cache-dir": "^6.0.0",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2"
  }
}
